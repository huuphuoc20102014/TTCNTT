@model ATAdmin.Efs.Entities.AspNetUsers
@{
    ViewData["Title"] = "Details";
}



<h4>Details Service</h4>
<hr />

<div class="row form-group">
    <div class="col-md-3 text-md-right ">
    </div>
    <div class="col-md-7">
        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
        <a asp-action="Delete" asp-route-id="@Model.Id" class="btn btn-danger">Delete</a>
        <a asp-action="Index" class="k-button">Back to List</a>
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Tài khoản :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.UserName)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        NormalizedUserName :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.NormalizedUserName)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Email :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.Email)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        NormalizedEmail :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.NormalizedEmail)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Xác nhận :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.EmailConfirmed)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right ">
    </div>
    <div class="col-md-7">
        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
        <a asp-action="Delete" asp-route-id="@Model.Id" class="btn btn-danger">Delete</a>
        <a asp-action="Index" class="k-button">Back to List</a>
    </div>
</div>
