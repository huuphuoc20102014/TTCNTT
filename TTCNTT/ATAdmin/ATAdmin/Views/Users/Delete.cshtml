@model ATAdmin.Efs.Entities.AspNetUsers
@{
    ViewData["Title"] = "Xóa dịch vụ";
}



<h4>Xóa dịch vụ</h4>
<h5 class="text-danger">Bạn có chắc muốn xóa?</h5>
<hr />

<form asp-action="Delete">
    <input type="hidden" name="Id" value="@Model?.Id" />
    <div class="row form-group">
        <div class="col-md-3 text-md-right ">
        </div>
        <div class="col-md-7">
            <input type="submit" value="Xóa" class="btn btn-danger" />
            <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Sửa</a>
            <a asp-action="Details" asp-route-id="@Model.Id" class="k-button">Chi tiết</a>
            <a asp-action="Index" class="k-button">Trở lại</a>
        </div>
    </div>
</form>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Tài khoản :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.UserName)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        NormalizedUserName :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.NormalizedUserName)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Email :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.Email)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        NormalizedEmail :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.NormalizedEmail)
    </div>
</div>

<div class="row form-group">
    <div class="col-md-3 text-md-right details-label">
        Xác nhận :
    </div>
    <div class="col-md-7">
        @Html.DisplayFor(model => model.EmailConfirmed)
    </div>
</div>

<form asp-action="Delete">
    <input type="hidden" name="Id" value="@Model?.Id" />
    <div class="row form-group">
        <div class="col-md-3 text-md-right ">
        </div>
        <div class="col-md-7">
            <input type="submit" value="Xóa" class="btn btn-danger" />
            <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Sửa</a>
            <a asp-action="Details" asp-route-id="@Model.Id" class="k-button">Chi tiết</a>
            <a asp-action="Index" class="k-button">Trở lại</a>
        </div>
    </div>
</form>


